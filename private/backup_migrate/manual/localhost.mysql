INSERT INTO `locales_source` VALUES ('1713','','default','Testing','','7.27'),('1714','','default','Download additional <a href="@modules">contributed modules</a> to extend Drupal''s functionality.','','7.27'),('1715','','default','Regularly review and install <a href="@updates">available updates</a> to maintain a secure and current site. Always run the <a href="@update-php">update script</a> each time a module is updated.','','7.27'),('1716','','default','Required by: !module-list','','7.27'),('1717','','default','Requires: !module-list','','7.27'),('1718','','default','Your name','','7.27'),('1719','','default','The content of this field is kept private and will not be shown publicly.','','7.27'),('1720','','default','Homepage','','7.27'),('1721','','default','Enter the terms you wish to search for.','','7.27'),('1722','','default','Secondary menu','','7.27'),('1723','','default','Search form','','7.27'),('1724','','default','Configure block','','7.27'),('1725','','default','Add new comment','','7.27'),('1726','','default','Administrative toolbar','','7.27'),('1727','','default','Hello <strong>@username</strong>','','7.27'),('1728','','default','Edit shortcuts','','7.27'),('1729','','default','anonymous user','','7.27'),('1730','/museusbr/admin/people/permissions?render=overlay','default','Access administration menu','','7.27'),('1731','/museusbr/admin/people/permissions?render=overlay','default','Display the administration menu at the top of each page.','','7.27'),('1732','/museusbr/admin/people/permissions?render=overlay','default','Flush caches','','7.27'),('1733','/museusbr/admin/people/permissions?render=overlay','default','Access links to flush caches in the administration menu.','','7.27'),('1734','/museusbr/admin/people/permissions?render=overlay','default','Display Drupal links','','7.27'),('1735','/museusbr/admin/people/permissions?render=overlay','default','Provide Drupal.org links in the administration menu.','','7.27'),('1736','/museusbr/admin/people/