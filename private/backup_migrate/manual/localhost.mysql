INSERT INTO `locales_source` VALUES ('7097','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','Disabling SQL rewriting will disable node_access checks as well as other modules that implement hook_query_alter().','','7.27'),('7098','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','WARNING: Disabling SQL rewriting means that node access security is disabled. This may allow users to see data they should not be able to see if your view is misconfigured. Please use this option only if you understand and accept this security risk.','','7.27'),('7099','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','Distinct','','7.27'),('7100','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','This will make the view display only distinct items. If there are multiple identical items, each will be displayed only once. You can use this to try and remove duplicates from a view, though it does not always work. Note that this can slow queries down, so use it with caution.','','7.27'),('7101','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','Pure Distinct','','7.27'),('7102','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','This will prevent views from adding the base column to the distinct field. If this is not selected and the base column is a primary key, then a non-pure distinct will not function properly because the primary key is always unique.','','7.27'),('7103','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','Use Slave Server','','7.27'),('7104','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','This will make the query attempt to connect to a slave server if available.  If no slave server is defined or available, it will fall back to the default server.','','7.27'),('7105','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','Query Comment','','7.27'),('7106','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','If set, this comment will be embedded in the query and passed to the SQL server. This can be helpful for logging or debugging.','','7.27');
INSERT INTO `locales_source` VALUES ('7107','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','Query Tags','','7.27'),('7108','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/query','default','If set, these tags will be appended to the query and can be used to identify the query in a module. This can be helpful for altering queries.','','7.27'),('7109','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/display_comment','default','This display''s comments','','7.27'),('7110','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/display_comment','default','This value will be seen and used only within the Views UI and can be used to document this display. You can use this to provide notes for other or future maintainers of your site about how or why this display is configured.','','7.27'),('7111','/museubr/pt-br/admin/structure/views/ajax/add-item/museus/page_1/empty','default','The label for this area that will be displayed only administratively.','','7.27'),('7112','/museubr/pt-br/admin/structure/views/ajax/add-item/museus/page_1/empty','default','Use replacement tokens from the first row','','7.27'),('7113','/museubr/pt-br/admin/structure/views/ajax/add-item/museus/page_1/empty','default','The following tokens are available. If you would like to have the characters ''['' and '']'' please use the html entity codes ''%5B'' or  ''%5D'' or they will get replaced with empty space.</p>','','7.27'),('7114','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/pager_options','default','Pager options','','7.27'),('7115','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/pager_options','default','The number of items to display per page. Enter 0 for no limit.','','7.27'),('7116','/museubr/pt-br/admin/structure/views/ajax/display/museus/page_1/pager_options','default','The number of items to skip. For example, if this field is 3, the first 3 items will be skipped and not displayed.