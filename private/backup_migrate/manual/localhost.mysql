INSERT INTO `locales_source` VALUES ('4487','','default','An unrecoverable error occurred. Use of this form has expired. Try reloading the page and submitting again.','','none'),('4488','','default','The file referenced by the !name field does not exist.','','none'),('4489','','default','The file in the !name field was unable to be uploaded.','','none'),('4490','','default','The upload directory %directory for the file field !name could not be created or is not accessible. A newly uploaded file could not be saved in this directory as a consequence, and the upload was canceled.','','none'),('4491','','default','Your server is not capable of displaying file upload progress. File upload progress requires an Apache server running PHP with mod_php.','','none'),('4492','','default','All roles may use this format','','none'),('4493','','default','The text format ordering has been saved.','','none'),('4494','','default','All roles for this text format must be enabled and cannot be changed.','','none'),('4495','','default','Filter processing order','','none'),('4496','','default','Text format names must be unique. A format named %name already exists.','','none'),('4497','','default','Added text format %format.','','none'),('4498','','default','The text format %format has been updated.','','none'),('4499','','default','A text format contains filters that change the user input, for example stripping out malicious HTML or making URLs clickable. Filters are executed from top to bottom and the order is important, since one filter may prevent another filter from doing its job. For example, when URLs are converted into links before disallowed HTML tags are removed, all links may be removed. When this happens, the order of filters may need to be re-arranged.','','none'),('4500','','default','A list of HTML tags that can be used. JavaScript event attributes, JavaScript URLs, and CSS are always stripped.','','none'),('4501','','default','Display basic HTML help in long filter tips',''