INSERT INTO `locales_source` VALUES ('5607','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Layer Attribution','','7.27'),('5608','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Allows layers to provide attribution to the map if it exists.  Most third-party layer will have some sort of attribution, but this may come with the actual tiles as well.','','7.27'),('5609','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Bounding Box Select','','7.27'),('5610','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Allows the selection and display of a bounding box.  Currently hard-coded for the map making interface.','','7.27'),('5611','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Cluster Features','','7.27'),('5612','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Provides vector layer features clustering by proximity.  This will not always respect the styles that assigned to the layer.','','7.27'),('5613','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Mouse Drag Pan','','7.27'),('5614','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Provides the ability to pan in the map interface with the mouse.  The Navigation Control includes this control.  You should only use this behavior if you want fine control over what interactions you want on your map.','','7.27'),('5615','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Draw Features','','7.27'),('5616','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Provides functionality for adding features to a map.','','7.27'),('5617','/museusbr/admin/config/development/performance?render=overlay&render=overlay','default','Fullscreen',