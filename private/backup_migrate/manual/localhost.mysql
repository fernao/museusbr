INSERT INTO `locales_source` VALUES ('4645','','default','October','Long month name','none'),('4646','','default','November','Long month name','none'),('4647','','default','December','Long month name','none'),('4648','','default','There are no menu links yet. <a href="@link">Add link</a>.','','none'),('4649','','default','The text to be used for this link in the menu.','','none'),('4650','','default','The path for this menu link. This can be an internal Drupal path such as %add-node or an external URL such as %drupal. Enter %front to link to the front page.','','none'),('4651','','default','Menu links that are not enabled will not be listed in any menu.','','none'),('4652','','default','Show as expanded','','none'),('4653','','default','If selected and this menu link has children, the menu will always appear expanded.','','none'),('4654','','default','Parent link','','none'),('4655','','default','The maximum depth for a link and all its children is fixed at !maxdepth. Some menu links may not be available as parents if selecting them would exceed this limit.','','none'),('4656','','default','Optional. In the menu, the heavier links will sink and the lighter links will be positioned nearer the top.','','none'),('4657','','default','Are you sure you want to delete the custom menu link %item?','','none'),('4658','','default','The menu link %title has been deleted.','','none'),('4659','','default','Are you sure you want to reset the link %item to its default values?','','none'),('4660','','default','The menu link was reset to its default settings.','','none'),('4661','','default','No Main links','','7.27'),('4662','','default','Source for the Main links','','7.27'),('4663','','default','Select what should be displayed as the Main links (typically at the top of the page).','','7.27'),('4664','','default','No Secondary links','','7.27'),('4665','','default','Source for the Secondary links','','7.27'),('4666','','default','Select the source for the Secondary links. An advanced option allows you to use the same source for both Main links (currently %main) and Secondary links: if your source menu has two levels of hierarchy, the top level menu links will appear in the Main links, and the children of the active link will appear in the Secondary links.','','none');
INSERT INTO `locales_source` VALUES ('4667','','default','Deleted custom menu %title and all its menu links.','','none'),('4668','','default','Deleted menu link %title.','','none'),('4669','','default','<strong>Warning:</strong> There is currently 1 menu link in %title. It will be deleted (system-defined items will be reset).','','none'),('4670','','default','<strong>Warning:</strong> There are currently @count menu links in %title. They will be deleted (system-defined links will be reset).','','none'),('4671','','default','You can enable the newly-created block for this menu on the <a href="@blocks">Blocks administration page</a>.','','none'),('4672','','default','Edit menu link','','none'),('4673','','default','Reset menu link','','none'),('4674','','default','Delete menu link','','none'),('4675','','default','The <em>Navigation</em> menu contains links intended for site visitors. Links are added to the <em>Navigation</em> menu automatically by some modules.','','none'),('4676','','default','The <em>User</em> menu contains links related to the user''s account, as well as the ''Log out'' link.','','none'),('4677','','default','The <em>Management</em> menu contains links for administrative tasks.','','none'),('4678','','default','The <em>Main</em> menu is used on many sites to show the major sections of the site, often in a top navigation bar.','','none'),('4679','','default','Invalid machine-readable name. Enter a name other than %invalid.','','none'),('4680','','default','%type is used by 1 piece of content on your site. If you remove this content type, you will not be able to edit the %type content and it may not display correctly.','','7.27'),('4681','','default','%type is used by @count pieces of content on your site. If you remove %type, you will not be able to edit the %type content and it may not display correctly.','','none'),('4682','','default','Deleted @count posts.','','none'),('4683','','default','Are you sure you want to delete this item?','','none'),