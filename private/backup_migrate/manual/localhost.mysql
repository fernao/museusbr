INSERT INTO `locales_source` VALUES ('3882','','default','The name of the field. The form name is not shown to the user but used internally in the HTML code and URLs.\nUnless you know what you are doing, it is highly recommended that you prefix the form name with <code>profile_</code> to avoid name clashes with other fields. Spaces or any other special characters except dash (-) and underscore (_) are not allowed. An example name is "profile_favorite_color" or perhaps just "profile_color".','','none'),('3883','','default','A list of all options. Put each option on a separate line. Example options are "red", "blue", "green", etc.','','none'),('3884','','default','Hidden profile field, only accessible by administrators, modules and themes.','','none'),('3885','','default','Private field, content only available to privileged users.','','none'),('3886','','default','Public field, content shown on profile page but not used on member list pages.','','none'),('3887','','default','Public field, content shown on profile page and on member list pages.','','none'),('3888','','default','The weights define the order in which the form fields are shown. Lighter fields "float up" towards the top of the category.','','none'),('3889','','default','Form will auto-complete while user is typing.','','none'),('3890','','default','For security, auto-complete will be disabled if the user does not have access to user profiles.','','none'),('3891','','default','The specified form name contains one or more illegal characters. Spaces or any other special characters except dash (-) and underscore (_) are not allowed.','','none'),('3892','','default','The specified form name is reserved for use by Drupal.','','none'),('3893','','default','The specified category name is reserved for use by Drupal.','','none'),('3894','','default','The specified title is already in use.','','none'),('3895','','default','The specified name is already in use.','','none'),('3896','','default','A hidden field cannot b