INSERT INTO `locales_source` VALUES ('1293','','default','Built-in interface','','7.27'),('1294','','default','English (built-in)','','7.27'),('1295','','default','n/a','','7.27'),('1296','','default','This page provides an overview of available translatable strings. Drupal displays translatable strings in text groups; modules may define additional text groups containing other translatable strings. Because text groups provide a method of grouping related strings, they are often used to focus translation efforts on specific areas of the Drupal interface.','','7.27'),('1297','','default','See the <a href="@languages">Languages page</a> for more information on adding support for additional languages.','','7.27'),('1298','','default','status','','7.27'),('1299','','default','any','','7.27'),('1300','','default','published','','7.27'),('1301','','default','not published','','7.27'),('1302','','default','promoted','','7.27'),('1303','','default','not promoted','','7.27'),('1304','','default','sticky','','7.27'),('1305','','default','not sticky','','7.27'),('1306','','default','Up to date translation','','7.27'),('1307','','default','Outdated translation','','7.27'),('1308','','default','type','','7.27'),('1309','','default','Language neutral','','7.27'),('1310','','default','language','','7.27'),('1311','','default','Show only items where','','7.27'),('1312','','default','Filter','','7.27'),('1313','','default','Update options','','7.27'),('1314','','default','Publish selected content','','7.27'),('1315','','default','Unpublish selected content','','7.27'),('1316','','default','Promote selected content to front page','','7.27'),('1317','','default','Demote selected content from front page','','7.27'),('1318','','default','Make selected content sticky','','7.27'),('1319','','default','Make selected content not sticky','','7.27'),('1320','','default','Delete selected content','','7.27'),('1321','','default','Operation','','7.27'),('1322','','default','Updated','','7.27');
