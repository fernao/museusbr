INSERT INTO `locales_source` VALUES ('1738','/museusbr/admin/people/permissions?render=overlay','default','Perform a backup','','7.27'),('1739','/museusbr/admin/people/permissions?render=overlay','default','Back up any of the available databases.','','7.27'),('1740','/museusbr/admin/people/permissions?render=overlay','default','Access backup files','','7.27'),('1741','/museusbr/admin/people/permissions?render=overlay','default','Access and download the previously created backup files.','','7.27'),('1742','/museusbr/admin/people/permissions?render=overlay','default','Delete backup files','','7.27'),('1743','/museusbr/admin/people/permissions?render=overlay','default','Delete the previously created backup files.','','7.27'),('1744','/museusbr/admin/people/permissions?render=overlay','default','Restore the site','','7.27'),('1745','/museusbr/admin/people/permissions?render=overlay','default','Restore the site''s database from a backup file.','','7.27'),('1746','/museusbr/admin/people/permissions?render=overlay','default','Administer Backup and Migrate','','7.27'),('1747','/museusbr/admin/people/permissions?render=overlay','default','Edit Backup and Migrate profiles, schedules and destinations.','','7.27'),('1748','','default','Administer blocks','','7.27'),('1749','/museusbr/admin/people/permissions?render=overlay','default','Access Bulk Exporter','','7.27'),('1750','/museusbr/admin/people/permissions?render=overlay','default','Export various system objects into code.','','7.27'),('1751','','default','Administer comments and comment settings','','7.27'),('1752','','default','View comments','','7.27'),('1753','','default','Post comments','','7.27'),('1754','','default','Skip comment approval','','7.27'),('1755','','default','Edit own comments','','7.27'),('1756','','default','Translate content','','7.27'),('1757','','default','Use contextual links','','7.27'),('1758','','default','Use contextual links to perform actions related to elements on a page.','','7.27'),('1759'